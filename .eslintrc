{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },

  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },

  "parser": "babel-eslint",

  "rules": {
    "eqeqeq": 1,
    "strict": 1,
    "curly": 2,
    "quotes": [2, "single"],
    "jsx-quotes": [1, "prefer-single"],
    "space-infix-ops": 1,
    "no-self-compare": 1,
    "no-with": 1,
    "default-case": 1,
    "no-shadow": 1,
    "no-undef-init": 1,
    "no-use-before-define": 1,
    "camelcase": 1
  }
}